{
  "name": "@parapara-dance/memory-database",
  "version": "0.0.0",
  "description": "In-memory resource database used by the website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test c8 mocha",
    "lint": "standardx --fix --verbose",
    "release": "release-it --config ./node_modules/@dimensionalpocket/release/config/default.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parapara-dance/memory-database-js.git"
  },
  "author": "dimensionalpocket.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parapara-dance/memory-database-js/issues"
  },
  "homepage": "https://github.com/parapara-dance/memory-database-js#readme",
  "dependencies": {
    "@dimensionalpocket/api-client": "github:dimensionalpocket/api-client-js#0.3.0",
    "eventemitter3": "4.0.7"
  },
  "devDependencies": {
    "@dimensionalpocket/development": "github:dimensionalpocket/development-js#0.6.1",
    "@dimensionalpocket/release": "github:dimensionalpocket/release-js#0.2.4",
    "nock": "13.2.7"
  },
  "babel": {
    "extends": "./node_modules/@dimensionalpocket/development/babel"
  },
  "eslintConfig": {
    "extends": "./node_modules/@dimensionalpocket/development/eslint"
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot",
    "colors": true
  },
  "c8": {
    "extends": "./node_modules/@dimensionalpocket/development/c8/default.json",
    "include": [
      "index.js",
      "src"
    ]
  },
  "engines": {
    "node": ">= 16.14.2"
  }
}
